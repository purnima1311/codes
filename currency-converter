#include<stdio.h>
#include<conio.h>
int CurrencyConverter(int,int);
int main()
{
    int code;
    int amount,result;
    printf("Code:");
    scanf("%d",&code);
    printf("Amount:");
    scanf("%d",&amount);

    result = CurrencyConverter(code,amount);
    printf("%d",result);
    return 0;
}

int CurrencyConverter(int code,int amount)
{
    if(code == 1)
    {
        return amount*65;
    }
    else if(code == 2)
    {
        return amount*96;
    }
    else if(code == 3)
    {
        return amount*67;
    }
    else if(code == 4)
    {
        return amount*74;
    }
    else
    {
        return amount*11;
    }
}
